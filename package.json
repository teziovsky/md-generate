{
  "name": "md-generate",
  "version": "0.0.1",
  "description": "NPM Package which generate README.md based on package.json.",
  "main": "src/index.js",
  "bin": {
    "md-generate": "src/index.js"
  },
  "keywords": [
    "markdown",
    "readme",
    "template",
    "readme.md",
    "generator",
    "generate",
    "cli"
  ],
  "author": "Jakub Soboczy≈Ñski",
  "license": "MIT",
  "homepage": "https://github.com/teziovsky/md-generate#readme",
  "scripts": {
    "lint": "./node_modules/.bin/prettier --check '**/*.{js,json}'",
    "lint:fix": "./node_modules/.bin/prettier --write '**/*.{js,json}'",
    "prepare": "husky install",
    "start": "node src/index.js"
  },
  "lint-staged": {
    "*.{js,json}": "prettier --write"
  },
  "dependencies": {
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1"
  }
}
